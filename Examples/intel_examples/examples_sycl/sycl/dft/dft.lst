# add main list variable name to DOMAINS_LIST variable
domains_list:= $(domains_list) dft

# main list of functions in this domain, use format
# <domain-folder-name>/<function_name_without_extension>
dft_common := \
dft/dp_complex_1d \
dft/sp_complex_1d \
dft/sp_real_1d \
dft/dp_real_1d \
dft/sp_real_1d_batch \
dft/sp_real_2d \
dft/dp_real_2d \
dft/sp_real_3d \
dft/dp_real_3d \
dft/sp_complex_2d \
dft/dp_complex_2d \
dft/sp_complex_2d_batch \
dft/dp_complex_2d_batch \
dft/sp_complex_3d \
dft/dp_complex_3d \
dft/sp_complex_3d_batch \
dft/dp_complex_3d_batch

# supports SYCL CPU device offloading
dft_cpu := $(dft_common)

# supports SYCL GPU device offloading
dft_gpu := $(dft_common)

# supports running on sycl host
dft_host := $(dft_common)

# sorts and removes duplicates from between cpu, gpu and host dft list
dft_all := $(sort $(dft_host) $(dft_cpu) $(dft_gpu))

