# add stats domain to domains_list variable
domains_list := $(domains_list) stats

# main list of functions in this domain, use format
# <domain-folder-name>/<function_name_without_extension>

stats_common := \
stats/minmax \
stats/minmax_usm \
stats/basic_statistics \
stats/basic_statistics_usm


# supports SYCL CPU device offloading
stats_cpu := $(stats_common)

# supports SYCL GPU device offloading
stats_gpu := $(stats_common)

# supports running on sycl host
stats_host := $(stats_common)

# sorts and removes duplicates from between cpu gpu and host stats lists
stats_all := $(stats_common)
