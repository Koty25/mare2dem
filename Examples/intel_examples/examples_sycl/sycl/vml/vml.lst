# add vml domain to domains_list variable
domains_list := $(domains_list) vml

# main list of functions in this domain, use format
# <domain-folder-name>/<function_name_without_extension>

vml_common := \
vml/vexp \
vml/vsincos_usm \
vml/vcdfnorminv_usm \
vml/vcis_usm \
vml/varg_usm \
vml/vpow_usm \
vml/vpowx_usm \
vml/vlinearfrac_usm \
vml/vexp_usm \
vml/vm_api_demo \
vml/vstatus

# supports SYCL CPU device offloading
vml_cpu := $(vml_common)

# supports SYCL GPU device offloading
vml_gpu := $(vml_common)

# supports running on sycl host
vml_host := $(vml_common)

# sorts and removes duplicates from between cpu gpu and host vml lists
vml_all := $(sort $(vml_gpu) $(vml_cpu) $(vml_host))

