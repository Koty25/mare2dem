# add lapack domain to domains_list variable
domains_list := $(domains_list) lapack

# main list of functions in this domain, use format
# <domain-folder-name>/<function_name_without_extension>

lapack_common := \
lapack/potrs \
lapack/potrs_usm \
lapack/geqrf \
lapack/getri \
lapack/lu_batch

# supports SYCL CPU device offloading
lapack_cpu := $(lapack_common)

# supports SYCL GPU device offloading
lapack_gpu := $(lapack_common)

# supports running on sycl host
lapack_host := $(lapack_common)

 # sorts and removes duplicates from between cpu, gpu and host lapack list
lapack_all := $(sort $(lapack_gpu) $(lapack_cpu) $(lapack_host))

