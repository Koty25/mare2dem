# add rng domain to domains_list variable
domains_list := $(domains_list) rng

# main list of functions in this domain, use format
# <domain-folder-name>/<function_name_without_extension>

rng_common := \
rng/uniform \
rng/uniform_usm \
rng/continious_distributions \
rng/discrete_distributions \
rng/soboluserdirnums-joe-kuo \
rng/service_functions

# supports SYCL CPU device offloading
rng_cpu := $(rng_common)

# supports SYCL GPU device offloading
rng_gpu := $(rng_common)

# supports running on sycl host
rng_host := $(rng_common)

# sorts and removes duplicates from between cpu gpu and host rng lists
rng_all := $(sort $(rng_gpu) $(rng_cpu) $(rng_host))

